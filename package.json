{
  "name": "ussf-portal",
  "version": "4.23.0",
  "private": true,
  "engines": {
    "node": "^18.14.2"
  },
  "repository": {
    "type": "git",
    "url": "github:USSF-ORBIT/ussf-portal-client"
  },
  "scripts": {
    "services:removeall": "docker ps --quiet --all | xargs docker rm --force",
    "services:up": "docker compose up -d matomo redis mongo-express idp keystone-db personnel-api",
    "services:down": "docker compose down",
    "cms:up": "docker compose up -d cms matomo mongo-express idp",
    "portal:up": "docker compose up --build -d cms client matomo mongo-express",
    "prebuild": "yarn generate",
    "dev": "concurrently \"yarn generate --watch\" \"node -r ./startup/index.js node_modules/.bin/next dev\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "node -r ./startup/index.js node_modules/.bin/next start",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest --runInBand --testTimeout=60000",
    "test:watch": "jest --watch  --runInBand --testTimeout=60000",
    "postinstall": "sh ./scripts/copy_uswds_assets.sh",
    "migrate:create": "migrate create --template-file ./migration-template.js",
    "generate": "graphql-codegen --config codegen.yml",
    "happo": "happo",
    "happo-ci-github-actions": "happo-ci-github-actions"
  },

{
  "name": "ussf-portal",
  "version": "2.4.0",
  "private": true,
  "engines": {
    "node": "14.19.0"
  },
  "repository": {
    "type": "git",
    "url": "github:USSF-ORBIT/ussf-portal-client"
  },
  "scripts": {
    "services:up": "docker compose -f docker-compose.services.yml up -d",
    "services:down": "docker compose -f docker-compose.services.yml down",
    "cms:up": "docker compose -f docker-compose.services.yml -f docker-compose.cms.yml up -d",
    "cms:down": "docker compose -f docker-compose.services.yml -f docker-compose.cms.yml down",
    "e2e:up": "docker compose -f docker-compose.services.yml -f docker-compose.e2e.dev.yml up -d",
    "e2e:down": "docker compose -f docker-compose.services.yml -f docker-compose.e2e.dev.yml down",
    "dev": "node -r ./startup/index.js node_modules/.bin/next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "node -r ./startup/index.js node_modules/.bin/next start",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest --runInBand --testTimeout=60000",
    "test:watch": "jest --watch  --runInBand --testTimeout=60000",
    "cypress:install": "yarn --cwd e2e/ install",
    "cypress:dev": "yarn --cwd e2e/ cypress open",
    "postinstall": "sh ./scripts/copy_uswds_assets.sh && keystone-next postinstall --fix",
    "release": "standard-version -t ''",
    "migrate:create": "migrate create --template-file ./migration-template.js"
  },
  "dependencies": {
    "@apollo/client": "3.5.9",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.17",
    "@keystone-next/fields": "15.0.0",
    "@keystone-next/keystone": "27.0.1",
    "@next/bundle-analyzer": "12.1.0",
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.27.0",
    "@opentelemetry/id-generator-aws-xray": "^1.0.1",
    "@opentelemetry/instrumentation-aws-sdk": "^0.5.1",
    "@opentelemetry/node": "^0.24.0",
    "@opentelemetry/propagator-aws-xray": "^1.0.1",
    "@opentelemetry/resource-detector-aws": "^1.0.3",
    "@opentelemetry/resources": "^1.0.1",
    "@opentelemetry/sdk-trace-node": "^1.0.1",
    "@opentelemetry/tracing": "^0.24.0",
    "@trussworks/react-uswds": "2.8.0",
    "apollo-server-core": "3.6.3",
    "apollo-server-micro": "3.6.3",
    "axios": "0.26.0",
    "bson": "4.6.1",
    "classnames": "2.3.1",
    "connect-redis": "6.1.1",
    "graphql": "16.3.0",
    "micro": "9.3.4",
    "migrate": "1.7.0",
    "mongodb": "3.7.3",
    "next": "12.1.0",
    "next-connect": "0.12.2",
    "next-session": "4.0.4",
    "passport": "0.5.2",
    "passport-saml": "3.2.1",
    "passport-saml-metadata": "2.5.0",
    "path-to-regexp": "6.2.0",
    "pino": "7.8.0",
    "react": "17.0.2",
    "react-cookie": "4.1.1",
    "react-dom": "17.0.2",
    "redis": "3.1.2",
    "uswds": "2.11.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@shelf/jest-mongodb": "2.2.0",
    "@storybook/addon-a11y": "6.4.19",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/react": "6.4.19",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/connect-redis": "0.0.18",
    "@types/jest-axe": "3.5.3",
    "@types/passport": "1.0.7",
    "@types/passport-saml-metadata": "2.2.1",
    "@types/react": "17.0.39",
    "@types/react-fontawesome": "1.6.5",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "babel-loader": "8.2.3",
    "css-loader": "5.2.7",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.2",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-security": "1.4.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-axe": "5.0.1",
    "lint-staged": "12.3.4",
    "next-test-api-route-handler": "3.1.1",
    "passport-custom": "1.1.1",
    "prettier": "2.5.1",
    "sass": "1.43.4",
    "sass-loader": "10.2.0",
    "standard-version": "9.3.2",
    "storybook-addon-apollo-client": "4.0.11",
    "storybook-addon-next-router": "3.1.1",
    "storybook-css-modules-preset": "1.1.1",
    "style-loader": "2.0.0",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "ansi-regex": "5.0.1",
    "graphql": "16.0.1",
    "trim": "0.0.3"
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "IE 11",
    "not dead"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true
    },
    "types": [
      {
        "type": "build",
        "hidden": true
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "ci",
        "hidden": true
      },
      {
        "type": "dev",
        "hidden": true
      },
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance Improvements"
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "revert",
        "section": "Reverted Changes"
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Documentation"
      }
    ]
  }
}

{
  "name": "ussf-portal",
  "version": "4.18.1",
  "private": true,
  "engines": {
    "node": "^18.14.2"
  },
  "repository": {
    "type": "git",
    "url": "github:USSF-ORBIT/ussf-portal-client"
  },
  "scripts": {
    "services:removeall": "docker ps --quiet --all | xargs docker rm --force",
    "services:up": "docker compose up -d matomo redis mongo-express idp keystone-db",
    "services:down": "docker compose down",
    "cms:up": "docker compose up -d cms matomo mongo-express idp",
    "portal:up": "docker compose up --build -d cms client matomo mongo-express",
    "prebuild": "yarn generate",
    "dev": "concurrently \"yarn generate --watch\" \"node -r ./startup/index.js node_modules/.bin/next dev\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "node -r ./startup/index.js node_modules/.bin/next start",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest --runInBand --testTimeout=60000",
    "test:watch": "jest --watch  --runInBand --testTimeout=60000",
    "postinstall": "sh ./scripts/copy_uswds_assets.sh",
    "release": "standard-version -t ''",
    "migrate:create": "migrate create --template-file ./migration-template.js",
    "generate": "graphql-codegen --config codegen.yml",
    "happo": "happo",
    "happo-ci-github-actions": "happo-ci-github-actions"
  },
  "dependencies": {
    "@apollo/client": "3.7.7",
    "@fortawesome/fontawesome-svg-core": "6.3.0",
    "@fortawesome/free-solid-svg-icons": "6.3.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@keystone-6/document-renderer": "1.1.2",
    "@keystone-6/fields-document": "5.0.2",
    "@next/bundle-analyzer": "12.3.4",
    "@opentelemetry/api": "1.1.0",
    "@opentelemetry/auto-instrumentations-node": "0.36.2",
    "@opentelemetry/auto-instrumentations-web": "0.31.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.35.1",
    "@opentelemetry/id-generator-aws-xray": "1.1.1",
    "@opentelemetry/node": "0.24.0",
    "@opentelemetry/propagator-aws-xray": "1.2.0",
    "@opentelemetry/resource-detector-aws": "1.2.2",
    "@opentelemetry/resources": "1.9.1",
    "@opentelemetry/sdk-trace-node": "1.9.1",
    "@opentelemetry/tracing": "0.24.0",
    "@trussworks/react-uswds": "3.2.0",
    "apollo-server-core": "3.11.1",
    "apollo-server-micro": "3.11.1",
    "axios": "1.3.2",
    "classnames": "2.3.2",
    "connect-redis": "6.1.3",
    "cookie-signature": "1.2.0",
    "dotenv": "16.0.3",
    "formik": "2.2.9",
    "graphql": "16.6.0",
    "luxon": "3.2.1",
    "launchdarkly-js-client-sdk": "3.1.0",
    "launchdarkly-react-client-sdk": "3.0.1",
    "micro": "10.0.1",
    "migrate": "1.8.0",
    "mongodb": "4.0.0",
    "next": "12.3.4",
    "next-connect": "0.13.0",
    "next-session": "4.0.5",
    "next-themes": "0.2.1",
    "passport": "0.5.2",

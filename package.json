{
  "name": "ussf-portal",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.17.4"
  },
  "repository": {
    "type": "git",
    "url": "github:USSF-ORBIT/ussf-portal-client"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "next build && next export",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "start:static": "serve out",
    "storybook": "start-storybook -p 6006 -s ./public",
    "storybook:build": "build-storybook",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "cypress:dev": "cypress open",
    "postinstall": "sh ./scripts/copy_uswds_assets.sh",
    "release": "standard-version -t ''"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^11.0.1",
    "@trussworks/react-uswds": "^2.0.0",
    "classnames": "^2.3.1",
    "next": "11.0.1",
    "path-to-regexp": "^6.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "uswds": "2.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@storybook/addon-a11y": "^6.3.0",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/react": "17.0.13",
    "@types/react-fontawesome": "^1.6.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "cypress": "^7.6.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "sass": "^1.35.1",
    "sass-loader": "^10.0.0",
    "serve": "^12.0.0",
    "standard-version": "^9.3.1",
    "storybook-addon-next-router": "^2.0.4",
    "storybook-css-modules-preset": "^1.1.1",
    "style-loader": "2.0.0",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true
    },
    "types": [
      {
        "type": "build",
        "hidden": true
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "ci",
        "hidden": true
      },
      {
        "type": "dev",
        "hidden": true
      },
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance Improvements"
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "revert",
        "section": "Reverted Changes"
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Documentation"
      }
    ]
  }
}

    "react": "18.2.0",
    "react-beautiful-dnd": "13.1.1",
    "react-cookie": "4.1.1",
    "react-dom": "18.2.0",
    "react-slick": "0.29.0",
    "redis": "3.1.2",
    "slick-carousel": "1.8.1",
    "uswds": "2.13.3",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",

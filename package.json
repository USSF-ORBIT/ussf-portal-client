{
  "name": "ussf-portal",
  "version": "2.0.0-alpha.0",
  "private": true,
  "engines": {
    "node": "14.18.1"
  },
  "repository": {
    "type": "git",
    "url": "github:USSF-ORBIT/ussf-portal-client"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s ./public",
    "storybook:build": "build-storybook",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest --runInBand --testTimeout=60000",
    "test:watch": "jest --watch  --runInBand --testTimeout=60000",
    "cypress:install": "yarn --cwd e2e/ install",
    "cypress:dev": "yarn --cwd e2e/ cypress open",
    "postinstall": "sh ./scripts/copy_uswds_assets.sh && keystone-next postinstall --fix",
    "release": "standard-version -t ''"
  },
  "dependencies": {
    "@apollo/client": "3.4.11",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@keystone-next/fields": "15.0.0",
    "@keystone-next/keystone": "27.0.1",
    "@next/bundle-analyzer": "12.0.2",
    "@trussworks/react-uswds": "2.4.1",
    "classnames": "2.3.1",
    "graphql": "16.0.1",
    "mongodb": "4.1.4",
    "next": "12.0.2",
    "next-connect": "0.11.0",
    "passport": "0.5.0",
    "passport-saml": "3.2.0",
    "passport-saml-metadata": "2.5.0",
    "path-to-regexp": "6.2.0",
    "react": "17.0.2",
    "react-cookie": "4.1.1",
    "react-dom": "17.0.2",
    "uswds": "2.11.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@storybook/addon-a11y": "6.3.12",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/react": "6.3.12",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest-axe": "3.5.3",
    "@types/passport": "1.0.7",
    "@types/passport-saml-metadata": "2.2.1",
    "@types/react": "17.0.34",
    "@types/react-fontawesome": "1.6.5",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "axe-core": "4.3.5",
    "babel-loader": "8.2.3",
    "css-loader": "5.2.7",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "jest-axe": "5.0.1",
    "lint-staged": "11.2.6",
    "mkdirp": "1.0.4",
    "next-test-api-route-handler": "^2.3.2",
    "prettier": "2.4.1",
    "sass": "1.43.4",
    "sass-loader": "10.2.0",
    "standard-version": "9.3.2",
    "storybook-addon-apollo-client": "4.0.9",
    "storybook-addon-next-router": "3.0.8",
    "storybook-css-modules-preset": "1.1.1",
    "style-loader": "2.0.0",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "ansi-regex": "5.0.1",
    "nth-check": "2.0.1",
    "object-path": "0.11.8",
    "tar": "6.1.11",
    "tmpl": "1.0.5",
    "trim": "0.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true
    },
    "types": [
      {
        "type": "build",
        "hidden": true
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "ci",
        "hidden": true
      },
      {
        "type": "dev",
        "hidden": true
      },
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance Improvements"
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "revert",
        "section": "Reverted Changes"
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "docs",
        "section": "Documentation"
      }
    ]
  }
}
